    </Compile>
    <Compile Update="HashType.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>HashType.tt</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
